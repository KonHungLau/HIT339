@model AnyoneForTennis.Models.ViewMembersViewModel

<h2>All Members</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Biography</th>
            <!-- Other fields as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.Users)
        {
            <tr>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.Email</td>
                <td>@user.Biography</td>
                <!-- Other fields as needed -->
            </tr>
        }
    </tbody>
</table>
